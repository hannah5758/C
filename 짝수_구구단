#include <stdio.h>

int main(void) {
    int num, result = 1;
    int a, b;
    
    for (a=1; a<=9; a++){
        if (a%2==0) {
            printf("\n%d 단 시작!\n", a);
            
            for (b=1; b<=9; b++){
                result = a * b;
                printf("%d x %d = %d \n", a, b, result);
            }
        }
    }
    
    return 0;
    
}
